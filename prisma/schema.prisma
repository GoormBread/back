generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Game {
  game_id String @id @default(uuid())
  player_num Int
  game_name String
  game_info String
  game_command Json
}

model User {
  user_id String @id
  password String
  nickname String
  user_game_command Json
}